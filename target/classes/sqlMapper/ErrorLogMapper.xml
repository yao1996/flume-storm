<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="info.ykqfrost.mapper.ErrorLogMapper">
    <resultMap id="BaseResultMap" type="info.ykqfrost.bean.ErrorLog">
        <id property="errorId" column="error_id"/>
        <result property="date" column="date"/>
        <result property="msg" column="msg"/>
        <result property="location" column="location"/>
    </resultMap>

    <insert id="insertError" parameterType="info.ykqfrost.bean.ErrorLog">
        INSERT INTO log.log_error
        VALUES (#{errorId}, #{date}, #{msg}, #{location})
    </insert>

</mapper>